<script>
  function buttonStickyMobile(){
    const getButtonBox = document.querySelector('.bottom-content-buttons');
    const getButtonPosition = getButtonBox.offsetTop;
    
    function addRemoveSticky(){
      if (window.pageYOffset >= getButtonPosition) {
        getButtonBox.classList.add('bottom-content-buttons-sticky');
      } else {
        getButtonBox.classList.remove('bottom-content-buttons-sticky');
      }    
    }

    if (window.matchMedia("(max-width: 991px)").matches && getButtonBox != null) {
      window.onscroll = function() {addRemoveSticky()};
    }
  }
  window.addEventListener("load", buttonStickyMobile);
</script>

